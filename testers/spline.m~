clear all;

x = [100,140,300]';
y = [100,150,100]';

% cosInterp


for point = 2:3
    
    xPlot = x(point-1):x(point);
    n = x(point) - x(point-1);
    
    for i = 1:n+1
        
        mew(i) = ((xPlot(i)) - x(point-1)) / (x(point) - x(point-1));
        mew2(i) = (1-cos(mew(i)*pi))/2;
        yPlot(i) = (y(point-1)*(1-mew2(i))+y(point)*mew2(i));
        
    end
    
end

plot(xPlot,yPlot);
